        "skip": "",
        "x": 400,
        "y": 100,
        "wires": [
            [
                "file-node"
            ]
        ]
    },
    {
        "id": "file-node",
        "type": "file",
        "z": "flow",
        "name": "Salvar em CSV",
        "filename": "/data/login-records.csv",
        "appendNewline": true,
        "createDir": false,
        "overwriteFile": "false",
        "encoding": "utf8",
        "x": 600,
        "y": 100,
        "wires": []
    },
    {
        "id": "http-response-node",
        "type": "http response",
        "z": "flow",
        "name": "",
