        "y": 100,
        "wires": [
            [
                "csv-node",
                "http-response-node"
            ]
        ]
    },
    {
        "id": "csv-node",
        "type": "csv",
        "z": "flow",
        "name": "Converter para CSV",
        "sep": ",",
        "hdrin": false,
